{
  "name": "skald-calculator",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "extract": "node extract-resources.js",
    "db-types": "supabase gen types typescript --local > ./src/lib/supabase/types/database.generated.ts",
    "db-up": "supabase migrations up && pnpm db-types",
    "db-reset": "supabase db reset && pnpm db-types",
    "supabase-restart": "supabase stop --no-backup && supabase start",
    "pkg-up": "pnpm up -i --latest"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.44.4",
    "@tanstack/react-router": "^1.45.10",
    "ajv": "^8.17.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "react": "^18.2.0",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "reactflow": "^11.11.4",
    "tailwind-merge": "^2.4.0",
    "use-debounce": "^10.0.1",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "1.45.10",
    "@tanstack/router-plugin": "^1.45.8",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "json-schema-to-ts": "^3.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "supabase": "^1.187.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
}
